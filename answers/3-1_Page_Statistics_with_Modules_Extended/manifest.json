{
  "manifest_version": 2,
  "name": "Page statistics",
  "description": "Show your visit statistics",
  "version": "0.1",
  "background": {
    "page": "background.html"
  },
  "content_scripts": [
    {
      "matches": [ "<all_urls>" ],
      "js": [ "content.js" ]
    }
  ],
  "permissions": [
    "tabs",
    "history",
    "bookmarks"
  ]
}

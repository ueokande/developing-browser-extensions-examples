{
  "manifest_version": 2,
  "name": "Force dark mode",
  "description": "Force background and foreground colors",
  "version": "0.1",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "tabs",
    "storage",
    "<all_urls>"
  ],
  "options_ui": {
    "page": "options.html"
  }
}

{
  "manifest_version": 2,
  "name": "Tab list popup",
  "description": "Show tabs on the popup",
  "version": "0.1",
  "browser_action": {
    "default_title": "Tab list popup",
    "default_popup": "popup.html",
    "default_icon": "browser_action_icon.png"
  },
  "permissions": [
    "tabs"
  ]
}
